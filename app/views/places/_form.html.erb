<div class="row">
	<%= form_for @place, url: {action: "create"}, html: {class: "place_form"} do |f| %>
		<%= label_tag(:alias, "hur.io/:") %>
		<%= f.text_field :alias, :placeholder => "mycrib" %>
	  <%= f.hidden_field :name, id: "chosen-place-name", :placeholder => "(Optional) Name" %>
	  <%= f.hidden_field :google_friendly_name, id: "chosen-place-google-friendly-name-field", :placeholder => "Google Friendly Name" %>
	  <%= f.hidden_field :formatted_address, id: "chosen-place-formatted-address-field", :placeholder => "(Optional) Address" %>
	  <%= f.hidden_field :lat, id: "chosen-place-lat", :placeholder => "Latitude" %>
	  <%= f.hidden_field :long, id: "chosen-place-lng", :placeholder => "Longitude" %>
		<%= label_tag(:notes, "Notes:") %>
	  <%= f.text_area :notes, id: "chosen-place-notes-field", :placeholder => "(Optional) Notes" %>
	  <%= label_tag(:parking_notes, "Parking notes:") %>
	  <%= f.text_area :parking_notes, id: "chosen-place-parking-notes-field", :placeholder => "(Optional) Parking Notes" %>
	  <%= f.submit "Create" %>
	<% end %>
</div>